msgid "<div class='entry'>Entry fee: <span>%(entry_fee)s€</span></div>"
msgstr "<div class='entry'>Dalības maksa: <span>%(entry_fee)s€</span></div>"

#: utils.py:85
#, python-format
msgid "<div class='insurance'>Insurance fee: <span>%(insurance_fee)s€</span></div>"
